{
  "expo": {
    "name": "Wayfar",
    "slug": "wayfar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "assetPatternsToBeBundled": ["assets/countries.json"]
    },
    "ios": {
      "bundleIdentifier": "me.lorenzo0111.wayfar",
      "supportsTablet": true
    },
    "android": {
      "package": "me.lorenzo0111.wayfar"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-asset"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "25846c0a-58ac-455b-8fde-fe3ea0e99f8e"
      }
    },
    "owner": "lorenzo0111"
  }
}
