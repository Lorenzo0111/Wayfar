{
  "expo": {
    "name": "Wayfar",
    "slug": "wayfar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E1E"
    },
    "scheme": "wayfar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "assetPatternsToBeBundled": [
        "assets/countries.json",
        "assets/regions.json"
      ]
    },
    "ios": {
      "appleTeamId": "829NR3MP39",
      "bundleIdentifier": "me.lorenzo0111.wayfar",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.security.application-groups": ["group.me.lorenzo0111.wayfar"]
      }
    },
    "android": {
      "package": "me.lorenzo0111.wayfar",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1E1E1E"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-asset",
        {
          "assets": ["assets/regions.json", "assets/regions.json"]
        }
      ],
      "@bacons/apple-targets"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "25846c0a-58ac-455b-8fde-fe3ea0e99f8e"
      }
    },
    "owner": "lorenzo0111"
  }
}
